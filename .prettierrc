{
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "printWidth": 80,
  "arrowParens": "avoid",
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "proseWrap": "preserve",
  "rangeStart": 0,
  "trailingComma": "es5",
  "useTabs": false,
  "overrides": [
    {
      "files": "helpers/tools/color/**/*.*",
      "options": {
        "printWidth": 120,
        "semi": true
      }
    }
  ]
}
