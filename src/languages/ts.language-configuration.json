{
  "comments": {
    "lineComment": "//",
    "blockComment": ["/*", "*/"]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    {
      "open": "${",
      "close": "}",
      "in": ["string"]
    },
    {
      "open": "$",
      "close": "",
      "in": ["string"]
    },
    {
      "open": "{",
      "close": "}"
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "'",
      "close": "'",
      "notIn": ["string", "comment"]
    },
    {
      "open": "<",
      "close": ">",
      "notIn": ["string", "comment"]
    },
    {
      "open": "\"",
      "close": "\"",
      "notIn": ["string"]
    },
    {
      "open": "`",
      "close": "`",
      "notIn": ["string", "comment"]
    },
    {
      "open": "/*",
      "close": " */",
      "notIn": ["string"]
    }
  ],
  "surroundingPairs": [
    ["${", "}"],
    ["$", " "],
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["'", "'"],
    ["\"", "\""],
    ["`", "`"],
    ["<", ">"],
    ["/", "/"],
    ["*", "*"],
    ["/*", "*/"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*//\\s*#?region\\b",
      "end": "^\\s*//\\s*#?endregion\\b"
    }
  }
}

