  "tokenColors": [
    {
      "scope": [
        "switch-block.expr",
        "switch-expression.expr",
        "switch-statement.expr"
      ],
      "settings": { "foreground": "#FEBF", "fontStyle": "normal" }
    },
    {
      "scope": [
        "TypeScript",
        "variable.language.arguments",
        "variable.object.property",
        "variable.object.property",
        "variable.other.constant.object.property",
        "variable.other.constant.object",
        "variable.other.constant.property",
        "variable.other.constant",
        "variable.other.description.jsdoc",
        "variable.other.enummember",
        "variable.other.jsdoc",
        "variable.other.link.underline.jsdoc",
        "variable.other.object.property",
        "variable.other.object",
        "variable.other.property",
        "variable.other.readwrite.alias",
        "variable.other.readwrite",
        "variable.other.regexp",
        "variable.parameter"
      ],
      "settings": { "foreground": "#B1F0FE", "fontStyle": "normal" }
    },

    {
      "scope": "constant.language.boolean.true",
      "settings": {
        "foreground": "#73BA3D",
        "fontStyle": "bold italic"
      }
    },
    {
      "scope": "constant.language.boolean.false",
      "settings": {
        "foreground": "#BA3D73",
        "fontStyle": "bold italic"
      }
    },
    {
      "scope": "constant.language.null",
      "settings": {
        "foreground": "#FA574C",
        "fontStyle": "normal"
      }
    },
    {
      "scope": "constant.language.undefined",
      "settings": {
        "foreground": "#B93AE5",
        "fontStyle": "normal"
      }
    },
    {
      "scope": "keyword.operator.new",
      "settings": {
        "foreground": "#FE8019",
        "fontStyle": "normal"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#7CE403"
      }
    },
    {
      "scope": ["constant.language.nan", "constant.language.infinity"],
      "settings": {
        "foreground": "#4D90E9",
        "fontStyle": "normal"
      }
    },
    {
      "scope": [
        "constant.language",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#ED659E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#F42C3E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#FE8019",
        "fontStyle": "normal"
      }
    },
    {
      "scope": "object",
      "settings": {
        "foreground": "#EBDBB2"
      }
    },
    {
      "scope": "module.node",
      "settings": {
        "foreground": "#99C6CA"
      }
    },

    {
      "name": "Arow function declarations",
      "scope": ["storage.type.function.arrow"],
      "settings": {
        "foreground": "#51D242",
        "fontStyle": "normal"
      }
    },
    {
      "name": "number",
      "scope": [
        "constant.numeric",
        "constant.numeric.binary",
        "constant.numeric.decimal",
        "constant.numeric.hex",
        "constant.numeric.octal"
      ],
      "settings": { "foreground": "#D24251", "fontStyle": "bold" }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#F9B516"
      }
    },
    {
      "scope": ["keyword.control.import", "keyword.control.export"],
      "settings": {
        "foreground": "#F42C3E",
        "fontStyle": "italic"
      }
    }
  ]
}
