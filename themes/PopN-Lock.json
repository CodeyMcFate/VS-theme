{
  "name": "Pop N Lock Theme by Luxcium",
  "type": "dark",

  "tokenColors": [
    {
      "scope": [
        "entity.function",
        "entity.name.function",
        "entity.name.function.static"
      ],
      "settings": {
        "foreground": "#F9B506"
      }
    },
    {
      "name": "Function declarations",
      "scope": ["storage.type.function", "storage.type.function.arrow"],
      "settings": {
        "foreground": "#7CA558",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Arrow function declarations",
      "scope": ["storage.type.function.arrow"],
      "settings": {
        "foreground": "#7CA558",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["storage.modifier.async"],
      "settings": { "foreground": "#FE8019", "fontStyle": "normal" }
    },
    {
      "scope": "storage.type.class",
      "settings": {
        "foreground": "#FE8019",
        "fontStyle": "normal"
      }
    },
    {
      "scope": [
        "keyword.control.as",
        "keyword.control.default",
        "keyword.control.import",
        "keyword.control.from",
        "keyword.control.export",
        "keyword.control.anchor.regexp",
        "keyword.control.conditional",
        "keyword.control.flow",
        "keyword.control.loop",
        "keyword.control.new",
        "keyword.control.require",
        "keyword.control.switch",
        "keyword.control.trycatch",
        "keyword.control.with",
        "keyword.control"
      ],
      "settings": { "foreground": "#F43C3E", "fontStyle": "normal" }
    },

    {
      "scope": [
        "storage.type",
        "keyword.control.import",
        "keyword.control.export"
