      "settings": {
        "foreground": "#F42C3E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.language.boolean.true",
      "settings": {
        "foreground": "#73BA3D",
        "fontStyle": "bold italic"
      }
    },
    {
      "scope": "constant.language.boolean.false",
      "settings": {
        "foreground": "#BA3D73",
        "fontStyle": "bold italic"
      }
    },
    {
      "scope": "constant.language.null",
      "settings": {
        "foreground": "#FA574C",
        "fontStyle": "normal"
      }
    },
    {
      "scope": "constant.language.undefined",
      "settings": {
        "foreground": "#B93AE5",
        "fontStyle": "normal"
      }
    },
    {
      "scope": "keyword.operator.new",
      "settings": {
        "foreground": "#7CE403",
        "fontStyle": "normal"
      }
    },

    {
      "scope": ["constant.language.nan", "constant.language.infinity"],
      "settings": {
        "foreground": "#4D90E9",
        "fontStyle": "normal"
      }
    },
    {
      "scope": [
        "constant.language",
        "variable.language.this",
        "variable.language.super"
