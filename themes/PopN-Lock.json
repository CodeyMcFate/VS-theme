      "settings": { "foreground": "#E65D", "fontStyle": "bold" }
    },

    {
      "scope": ["comment.line.double-slash"],
      "settings": { "foreground": "#459598BB", "fontStyle": "normal" }
    },
    {
      "scope": ["comment.block"],
      "settings": { "foreground": "#4565B8BB", "fontStyle": "normal" }
    },
    {
      "scope": ["comment.block.documentation"],
      "settings": { "foreground": "#AAA8", "fontStyle": "normal" }
